 		</navigation-case>  	
 	</navigation-rule>
 	
 	<navigation-rule>
		<from-view-id>item.jsf</from-view-id>
 		<navigation-case>
 			<from-outcome>index</from-outcome>
 			<to-view-id>index.jsf</to-view-id> 			
